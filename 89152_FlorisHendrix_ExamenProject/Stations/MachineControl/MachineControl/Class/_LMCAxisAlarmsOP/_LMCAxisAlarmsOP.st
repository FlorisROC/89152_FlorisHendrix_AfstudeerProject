        DriveErrorInfoState := _DRIVE_ERROR_STATE_DRIVE_NOT_CONNECTED;
      end_if;
      

      if IsClientConnected(input:=#LMCAXIS) then
      
        if IsClientConnected(input:= #DriveAxis) then
          AxisErrorInfoState := _AXIS_ERROR_STATE_INIT;
        else
          AxisErrorInfoState := _AXIS_ERROR_STATE_IDLE;
        end_if;

      else
        AxisErrorInfoState := _AXIS_ERROR_LMCAXIS_NOT_CONNECTED;
      end_if;

    end_if;

END_FUNCTION
  

