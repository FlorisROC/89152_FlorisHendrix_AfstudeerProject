FUNCTION _LMCAxisAlarmsOP::GetErrorInfo
VAR
  DriveError : DINT;
END_VAR

  // *******************************************************
  // ***               Set Axis Errors FSM               ***
  // *******************************************************

  case AxisErrorInfoState of
  
    // *****************************************************
    _AXIS_ERROR_STATE_INIT:    
          
      if DriveAxis.Online = 1 then
        AxisErrorInfoState := _AXIS_ERROR_STATE_IDLE;
      end_if;

    // *****************************************************
    _AXIS_ERROR_STATE_IDLE:
      LMCaxiserror := LMCAXIS.AxisError.Read();
      
      if LMCaxiserror > 0 then
        SetLMCAxisAlarm(AxisErrors:=LMCaxiserror);
        AxisErrorInfoState := _AXIS_ERROR_STATE_ERROR;
      end_if;

    // *****************************************************
    _AXIS_ERROR_STATE_ERROR:
    
      LMCaxiserror := LMCAXIS.AxisError.Read();
      
      if LMCaxiserror = 0 then
        SetLMCAxisAlarm(AxisErrors:=LMCaxiserror);
        AxisErrorInfoState := _AXIS_ERROR_STATE_IDLE;
      end_if;
      
    // *****************************************************
    _AXIS_ERROR_LMCAXIS_NOT_CONNECTED:

  end_case;
  

  // *******************************************************
  // ***            Set Drive Errors FSM                 ***
  // *******************************************************
  
  case DriveErrorInfoState of
    
    // *****************************************************
    _DRIVE_ERROR_STATE_INIT: 
    
      if DriveAxis.Online = 1 then
        DriveErrorInfoState := _DRIVE_ERROR_STATE_IDLE;
      end_if;
    
    // *****************************************************
    _DRIVE_ERROR_STATE_IDLE:
    
    DriveError := DriveAxis.AxError.Read();
    
      if DriveError > 0 then
        _DriveErrorHandling1.GetErrorInfo.Write(1);
        if _DriveErrorHandling1.State = BUSY then
          DriveErrorInfoState := _DRIVE_ERROR_STATE_WAIT_FOR_DRIVE_INFO;
        end_if;
      end_if;    
    
    // *******************************************************
    _DRIVE_ERROR_STATE_WAIT_FOR_DRIVE_ERROR_HANDLING:
    
      
      
    // *******************************************************
    _DRIVE_ERROR_STATE_WAIT_FOR_DRIVE_INFO:  
    
      if _DriveErrorHandling1.State = READY then
               
        vI_DERROR := _DriveErrorHandling1.I_DERROR;
        vI_STATUS := _DriveErrorHandling1.I_STATUS;
        
        // Drive Error
        SetSDDIDError(I_DError:=vI_DERROR);
           
        // Drive State
        SetSDDStatus(I_Status:=vI_STATUS);
        
        DriveErrorInfoState := _DRIVE_ERROR_STATE_ERROR;
      end_if;
    
    // *******************************************************
    _DRIVE_ERROR_STATE_ERROR:
      DriveError := DriveAxis.AxError.Read();
    
      if DriveError = 0 then
        SetSDDIDError(I_DERROR:= 0);
        SetSDDStatus(I_STATUS:= 0);
        DriveErrorInfoState := _DRIVE_ERROR_STATE_IDLE;
      end_if;
    
    // *******************************************************
    _DRIVE_ERROR_STATE_DRIVE_NOT_CONNECTED:
    
    
  end_case;
END_FUNCTION


